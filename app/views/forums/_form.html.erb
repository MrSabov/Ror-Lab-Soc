<%= form_for @forum, html: { multiparth: true} do |f| %>

    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :body %>
      <%= f.text_area :body, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.file_field :image %>
    </div>

    <div class="form-group">
      <%= f.submit 'Создать', class: 'btn btn-primary' %>
    </div>

<% end %>